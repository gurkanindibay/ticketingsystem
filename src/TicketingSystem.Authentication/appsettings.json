{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=ticketingdb;Username=ticketinguser;Password=ticketingpass123;"
  },
  "JwtSettings": {
    "SecretKey": "YourSuperSecretKeyThatIsAtLeast32CharactersLong12345",
    "Issuer": "TicketingSystem",
    "Audience": "TicketingSystem.Users",
    "AccessTokenExpirationMinutes": 15,
    "RefreshTokenExpirationDays": 7
  },
  "DatabaseSettings": {
    "EnableSensitiveDataLogging": false,
    "CommandTimeout": 30
  },
  "RedisSettings": {
    "ConnectionString": "localhost:6379",
    "Database": 0,
    "KeyPrefix": "auth:",
    "DefaultExpirationMinutes": 60
  },
  "RateLimiting": {
    "Registration": {
      "MaxAttempts": 5,
      "WindowMinutes": 60
    },
    "Login": {
      "MaxAttempts": 10,
      "WindowMinutes": 15
    },
    "Refresh": {
      "MaxAttempts": 20,
      "WindowMinutes": 5
    }
  }
}
